{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "baseUrl": "src/",
    "paths": {
      "Images": ["images"],
      "Components": ["components"],
      "Styles": ["globalStyles"],
      "@": [""]
    }
  },
  "include": ["src"]
}
